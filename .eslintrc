{
	"extends": "airbnb",
	"rules": {
		"no-tabs": "off",
		"indent": [
			2,
			"tab",
			{
				"SwitchCase": 1
			}
		],
		"jsx-a11y/no-marquee": "off",
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [
					".js",
					".jsx"
				]
			}
		],
		"react/jsx-indent": [
			2,
			"tab"
		],
		"react/jsx-indent-props": [
			2,
			"tab"
		],
		"react/no-array-index-key": "off",
		"jsx-a11y/no-static-element-interactions": "off"
	}
}
